{
  "redirects": [
    { "source": "/index.html", "destination": "/", "permanent": true },
    { "source": "/:slug.html", "destination": "/:slug", "permanent": true }
  ],
  "rewrites": [
    { "source": "/aged-care-podiatry", "destination": "/aged-care-podiatry.html" },
    { "source": "/home-visit-services", "destination": "/home-visit-services.html" },
    { "source": "/work-with-us", "destination": "/work-with-us.html" },
    { "source": "/contact-us", "destination": "/contact-us.html" },
    { "source": "/refer-to-us", "destination": "/refer-to-us.html" },
    { "source": "/privacy-policy", "destination": "/privacy-policy.html" },
    { "source": "/consumer-rights", "destination": "/consumer-rights.html" },
    { "source": "/legal-information", "destination": "/legal-information.html" }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(css|js)$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.(jpg|jpeg|png|gif|svg|webp)$",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}